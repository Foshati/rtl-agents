{
  "publisher": "Foshati",
  "name": "rtl-agents",
  "displayName": "RTL Agents",
  "version": "1.7.3",
  "packageManager": "pnpm@10.22.0",
  "description": "Simple RTL/LTR toggle",
  "author": "Foshati",
  "license": "MIT",
  "homepage": "https://github.com/Foshati/rtl-agents",
  "repository": {
    "type": "git",
    "url": "https://github.com/Foshati/rtl-agents"
  },
  "keywords": [
    "rtl",
    "ltr",
    "toggle"
  ],
  "categories": [
    "Other"
  ],
  "main": "./dist/index.js",
  "icon": "public/icon.png",
  "engines": {
    "vscode": "^1.80.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "commands": [
      {
        "command": "rtl-agents.toggle",
        "title": "RTL Agents: Toggle RTL/LTR"
      }
    ],
    "configuration": {
      "title": "RTL Agents",
      "properties": {
        "rtl-agents.mode": {
          "type": "string",
          "enum": [
            "rtl",
            "ltr"
          ],
          "default": "rtl",
          "description": "Current mode"
        }
      }
    },
    "keybindings": [
      {
        "command": "rtl-agents.toggle",
        "key": "ctrl+alt+r",
        "mac": "ctrl+cmd+r"
      }
    ]
  },
  "scripts": {
    "build": "tsup src/index.ts --external vscode",
    "lint": "eslint .",
    "vscode:prepublish": "pnpm build"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.7.3",
    "@types/node": "^25.0.3",
    "@types/vscode": "^1.80.0",
    "eslint": "^9.39.2",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  }
}
